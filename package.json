{
  "name": "chattrbox",
  "version": "1.0.0",
  "description": "first nodejs service",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error:no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "browserify -d app/scripts/src/main.js -o app/scripts/dist/main.js",
    "watch": "watchify -v -d app/scripts/src/main.js -o app/scripts/dist/main.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ],
          "sourceMap": true
        }
      ]
    ]
  },
  "keywords": [
    "chttrbox"
  ],
  "author": "rock an",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "crypto-js": "^4.0.0",
    "jquery": "^3.5.0",
    "moment": "^2.25.1",
    "nodemon": "^2.0.2",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "ws": "^7.4.6"
  }
}
